  "description": "Build out a random generating instanced zone to explore!",
  "authors": [
    "Boss Hog"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/zones/icon.png",

  "environment": "*",
